package javaStudy;

//스트링 클래스의 문제점
public class StringProblem {

	public static void main(String[] args) {
		String str1 = "";
		for(int i=0; i<100; i++) {
			str1 += "*";
		}
		System.out.println(str1);
		
		StringBuffer sb = new StringBuffer();
		for(int i=0; i<100; i++) {
			sb.append("*");
		}
		String str2 = sb.toString();
		System.out.println(str2);

	}

}
