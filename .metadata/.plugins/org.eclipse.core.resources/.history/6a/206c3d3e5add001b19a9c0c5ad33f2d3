package programmers;

import java.util.Arrays;

//가장큰수
public class Sort1 {

	public static void main(String[] args) {
		class Solution {
		    public String solution(int[] numbers) {
		        String answer = "";
		        int max = 0, idx = 0;
		        for(int i=1; i<numbers.length; i++) {
		        	for(int j=i; j>0; j--) {
			        	char cur = Integer.toString(numbers[j]).charAt(0);
			        	char pre = Integer.toString(numbers[j-1]).charAt(0);
		        		if(Character.getNumericValue(cur) > Character.getNumericValue(pre)) {
		        			int tmp = numbers[j];
		        			numbers[j] = numbers[j-1];
		        			numbers[j-1] = tmp;
		        		}
		        		else if(Character.getNumericValue(cur) == Character.getNumericValue(pre)) {
		        			//cur = Integer.toString(numbers[j]).charAt(1);
				        	//pre = Integer.toString(numbers[j-1]).charAt(1);
		        		}
		        	}
		        }
		        String s = "12345";
		        System.out.println(s.charAt50));
		        System.out.println(Arrays.toString(numbers));
		        
		        
		        return answer;
		    }
		}
		
		Solution s = new Solution();
		
		int[] numbers = {3, 30, 34, 5, 9};
 		s.solution(numbers);

	}

}
