package programmers;

//로또 최고 순위와 최저 순위
public class LottoLank {

	public static void main(String[] args) {
		class Solution {
		    public int[] solution(int[] lottos, int[] win_nums) {
		        int[] answer = {};
		        int lowcnt=0;
		        int highcnt=0;
		        
		        for(int x : lottos) {
		        	if(x==0) highcnt++;
		        	for(int y : win_nums) {
		        		if(x == y) {
		        			lowcnt++;
		        			highcnt++;
		        		}
		        	}
		        }
		        System.out.println(highcnt);
		        System.out.println(lowcnt);
		        return answer;
		    }
		}
		
		Solution s = new Solution();
		int[] lottos = {44, 1, 0, 0, 31, 25};
		int[] win_nums = {31, 10, 45, 1, 6, 19};
		s.solution(lottos, win_nums);

	}

}
