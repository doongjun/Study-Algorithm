package programmers;

import java.util.Arrays;

//가장큰수
public class Sort1 {

	public static void main(String[] args) {
		class Solution {
		    public String solution(int[] numbers) {
		        String answer = "";
		        System.out.println(Arrays.toString(numbers));
		        return answer;
		    }
		}
		
		Solution s = new Solution();
		
		int[] numbers = {6, 10, 2};
 		s.solution(numbers);

	}

}
