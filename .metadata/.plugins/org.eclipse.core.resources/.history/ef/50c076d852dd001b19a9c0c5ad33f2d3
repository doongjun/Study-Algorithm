package programmers;

import java.util.Arrays;

//가장큰수
public class Sort1 {

	public static void main(String[] args) {
		class Solution {
		    public String solution(int[] numbers) {
		        String answer = "";
		        int max = 0;
		        for(int i=0; i<numbers.length; i++) {
		        	char tmp = (char)(numbers[i] - '0');
		        	System.out.println(tmp);
		        }
		        
		        
		        return answer;
		    }
		}
		
		Solution s = new Solution();
		
		int[] numbers = {6, 10, 2};
 		s.solution(numbers);

	}

}
