package programmers;

import java.util.Arrays;

//가장큰수
public class Sort1 {

	public static void main(String[] args) {
		class Solution {
		    public String solution(int[] numbers) {
		        String answer = "";
		        int max = 0;
		        for(int x : numbers) {
		        	char tmp = Integer.toString(x).charAt(0);
		        	if(Character.getNumericValue(tmp) > 5)
		        		System.out.println(tmp);
		        }
		        
		        
		        return answer;
		    }
		}
		
		Solution s = new Solution();
		
		int[] numbers = {6, 10, 2};
 		s.solution(numbers);

	}

}
