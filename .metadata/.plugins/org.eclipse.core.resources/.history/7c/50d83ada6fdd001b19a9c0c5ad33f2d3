package programmers;

//다음 큰 숫자
public class NextBigNum {

	public static void main(String[] args) {
		class Solution {
		    public int solution(int n) {
		        int answer = 0;
		        
		        System.out.println(Integer.toBinaryString(n));
		        
		        return answer;
		    }
		}
		
		Solution s = new Solution();
		s.solution(78);
	}

}
